{
    "connections": [
        {
            "name": "esper",
            "type": "irc",
            "connection": {
                "server": "irc.esper.net",
                "port": 6667,
                "ssl": false,
                "ignore_cert": true,
                "password": "",
                "timeout": 300
            },
            "ping_settings": {
                "interval": 60,
                "warn": 120,
                "timeout": 300
            },
            "nick": "MyCloudBot",
            "user": "cloudbot",
            "avoid_notices": false,
            "channels": [
                "#cloudbot",
                "#cloudbot2"
            ],
            "log_channel": "#MyAdminChannel",
            "disabled_commands": [],
            "acls": {},
            "sasl": {
                "enabled": false,
                "mechanism": "PLAIN",
                "user": "",
                "pass": ""
            },
            "nickserv": {
                "enabled": false,
                "nickserv_password": "",
                "nickserv_user": "",
                "nickserv_name": "nickserv",
                "nickserv_command": "IDENTIFY"
            },
            "ratelimit": {
                "max_tokens": 17.5,
                "restore_rate": 2.5,
                "message_cost": 5,
                "strict": true
            },
            "permissions": {
                "admins": {
                    "perms": [
                        "addfactoid",
                        "delfactoid",
                        "ignore",
                        "botcontrol",
                        "plpaste",
                        "permissions_users",
                        "op",
                        "manage_patron"
                    ],
                    "users": [
                        "examplea!user@example.com",
                        "exampleb!user@example.com"
                    ]
                },
                "moderators": {
                    "perms": [
                        "addfactoid",
                        "delfactoid",
                        "ignore"
                    ],
                    "users": [
                        "examplec!user@example.com"
                    ]
                }
            },
            "plugins": {
                "perkserv": {
                    "no_perk_msg": "Missing perk {perk_name}, please visit https://example.com"
                }
            },
            "command_prefix": "."
        }
    ],
    "plugins": {
    },
    "api_keys": {
    },
    "database": "sqlite:///cloudbot.db",
    "plugin_loading": {
        "use_whitelist": false,
        "blacklist": [
            "update"
        ],
        "whitelist": []
    },
    "reloading": {
        "config_reloading": true,
        "plugin_reloading": false
    },
    "repo_link": "https://github.com/snoonetIRC/CloudBot/",
    "logging": {
        "console_debug": false,
        "file_debug": false,
        "show_plugin_loading": true,
        "show_motd": true,
        "show_server_info": true,
        "raw_file_log": false,
        "file_log": true,
        "console_log_info": true
    }
}
